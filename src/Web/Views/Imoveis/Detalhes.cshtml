@using ImovelBens.CrossCutting.Identity.Models
@model ImovelBens.Domain.Entities.Imovel
@{
    ViewBag.Title = "Imovel";
    Layout = "~/Views/Shared/_LayoutSite.cshtml";

}

<div class="singleTop">
    <div id="carouselFull" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            @for (int i = 0; i < Model.Fotos.Count; i++)
            {
                if (i == 0)
                {
                    <li data-target="#carouselFull" data-slide-to="@i" class="active"></li>
                }

                else
                {
                    <li data-target="#carouselFull" data-slide-to="@i"></li>
                }


            }           
        </ol>
        <div class="carousel-inner">
            @for (int i = 0; i < Model.Fotos.Count; i++)
            {
                if (i == 0)
                {
                    <div class="item active">
                        <img src="/images/imoveis/@Model.Fotos.ElementAt(i).Nome" alt="First slide">
                        <div class="container">
                            <div class="carousel-caption">

                            </div>
                        </div>
                    </div>
                }
                else
                {
                    <div class="item">
                        <img src="/images/imoveis/@Model.Fotos.ElementAt(i).Nome" alt="Second slide">
                        <div class="container">
                            <div class="carousel-caption">

                            </div>
                        </div>
                    </div>
                }
            }

        </div>
        <a class="left carousel-control" href="#carouselFull" role="button" data-slide="prev"><span class="fa fa-chevron-left"></span></a>
        <a class="right carousel-control" href="#carouselFull" role="button" data-slide="next"><span class="fa fa-chevron-right"></span></a>
    </div>
    <div class="summary">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-8 col-lg-8">
                <div class="summaryItem">
                    <h1 class="pageTitle">@Html.DisplayFor(x => x.Title)</h1>
                    <div class="address"><span class="icon-pointer"></span> @Html.DisplayFor(x => x.Address)</div>
                    <ul class="rating"></ul>

                    <div class="clearfix"></div>
                    <ul class="features">
                        <li><span class="fa fa-moon-o"></span><div>@Html.DisplayFor(x => x.Bedrooms) Quarto(s)</div></li>
                        <li><span class="icon-drop"></span><div>@Html.DisplayFor(x => x.Bathrooms) Banheiro(s)</div></li>
                        <li><span class="icon-frame"></span><div>@Html.DisplayFor(x => x.Area) m2</div></li>
                    </ul>
                    <div class="clearfix"></div>
                </div>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                <div class="agentAvatar summaryItem">
                    <div class="clearfix"></div>
                    <img class="avatar agentAvatarImg" src="/images/avatar-2.png" alt="avatar">
                    <div class="agentName">@User.Identity.Name</div>
                    <a data-toggle="modal" href="#contactAgent" class="btn btn-lg btn-round btn-blue contactBtn">Entre em Contato</a>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="description">
    <h3>Descrição</h3>
    <p>@Html.DisplayFor(x => x.Description)</p>
</div>
<div class="share">
    <h3>Me encontre nas redes sociais</h3>
    <div class="row">
        <div class="col-xs-6 col-sm-6 col-md-3 col-lg-3 shareItem">
            <a href="#" class="btn btn-sm btn-round btn-o btn-facebook"><span class="fa fa-facebook"></span> Facebook</a>
        </div>
        <div class="col-xs-6 col-sm-6 col-md-3 col-lg-3 shareItem">
            <a href="#" class="btn btn-sm btn-round btn-o btn-twitter"><span class="fa fa-twitter"></span> Twitter</a>
        </div>
        <div class="col-xs-6 col-sm-6 col-md-3 col-lg-3 shareItem">
            <a href="#" class="btn btn-sm btn-round btn-o btn-google"><span class="fa fa-google-plus"></span> Google+</a>
        </div>
        <div class="col-xs-6 col-sm-6 col-md-3 col-lg-3 shareItem">
            <a href="#" class="btn btn-sm btn-round btn-o btn-pinterest"><span class="fa fa-pinterest"></span> Pinterest</a>
        </div>
    </div>
</div>
<div class="amenities">
    <h3>Amenities</h3>
    <div class="row">
        <div class="col-xs-6 col-sm-6 col-md-4 col-lg-4 amItem @Model.Garage.ToString()"><span class="fa fa-car"></span> Garagem</div>
        <div class="col-xs-6 col-sm-6 col-md-4 col-lg-4 amItem @Model.OutdoorPool.ToString()"><span class="fa fa-tint"></span> Piscina</div>
        <div class="col-xs-6 col-sm-6 col-md-4 col-lg-4 amItem @Model.Garden.ToString()"><span class="fa fa-leaf"></span> Jardim</div>
        <div class="col-xs-6 col-sm-6 col-md-4 col-lg-4 amItem @Model.SecuritySystem.ToString()"><span class="fa fa-shield"></span> Sistema de Segurança</div>
        <div class="col-xs-6 col-sm-6 col-md-4 col-lg-4 amItem @Model.Internet.ToString()"><span class="fa fa-wifi"></span> Internet</div>
        <div class="col-xs-6 col-sm-6 col-md-4 col-lg-4 amItem @Model.Telephone.ToString()"><span class="fa fa-phone"></span> Telefone</div>
        <div class="col-xs-6 col-sm-6 col-md-4 col-lg-4 amItem @Model.AirConditioning.ToString()"><span class="fa fa-asterisk"></span> Ar Condicionado</div>
        <div class="col-xs-6 col-sm-6 col-md-4 col-lg-4 amItem @Model.Heating.ToString()"><span class="fa fa-sun-o"></span> Aquecimento</div>
        <div class="col-xs-6 col-sm-6 col-md-4 col-lg-4 amItem @Model.FirePlace.ToString()"><span class="fa fa-fire"></span> Churrasqueira</div>
        <div class="col-xs-6 col-sm-6 col-md-4 col-lg-4 amItem @Model.Balcony.ToString()"><span class="fa fa-arrows-v"></span> Varanda</div>
        <div class="col-xs-6 col-sm-6 col-md-4 col-lg-4 amItem @Model.TvCable.ToString()"><span class="fa fa-desktop"></span> TV à Cabo</div>
    </div>
</div>
